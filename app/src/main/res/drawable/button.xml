<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#002B5B" /> <!-- Warna latar belakang tombol saat diklik -->
            <corners android:radius="8dp" /> <!-- Rounding sudut tombol (opsional) -->
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#159895" /> <!-- Warna latar belakang tombol biasa -->
            <corners android:radius="8dp" /> <!-- Rounding sudut tombol (opsional) -->
        </shape>
    </item>
</selector>
